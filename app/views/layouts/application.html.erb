<!DOCTYPE html>
<html>
<head>
  <title>SuperSurvey</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>
  <div class="stars">
    <div class="twinkling">
      <div class="container">
        <div class="jumbotron">
          <h1>Super Surveys!</h1>
        </div>
        <div class="card">

          <%= yield %>
        </div>
      </div>
    </div>
  </div>
</body>
</html>
